{#
/**
 * @file
 * Template for the main page.
 */
#}
<div class="page">
  {% embed '@radix/navbar/navbar.twig' with {
    placement: 'sticky-top',
    container: 'fixed',
    color: 'light',
    utility_classes: ['bg-light'],
  } %}


    {% block branding %}
      <div class="topbar">
<div class="header header1 po-relative">
<div class="container">
      {% if page.navbar_branding %}
        {{ page.navbar_branding }}
        </div>
</div>
</div>
      {% endif %}
    {% endblock %}


    {% block left %}
      {% if page.navbar_left %}

        
          {{ page.navbar_left }}
       
      {% endif %}
    {% endblock %}

    {% block right %}
      {% if page.navbar_right %}
        {{ page.navbar_right }}
      {% endif %}
    {% endblock %}
  {% endembed %}

{#   <main class="pt-5 pb-5">
    {% if page.header %}
      <header class="page__header mb-3 page_header_page">
